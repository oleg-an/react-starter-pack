{
  "name": "react-starter-pack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@headlessui/react": "1.7.13",
    "@lukemorales/query-key-factory": "1.2.0",
    "@popperjs/core": "2.11.7",
    "@sentry/react": "7.46.0",
    "@sentry/tracing": "7.46.0",
    "@tanstack/react-query": "4.29.13",
    "@tanstack/react-query-devtools": "4.29.13",
    "@types/debounce": "1.2.1",
    "@types/react-datepicker": "4.10.0",
    "add": "2.0.6",
    "axios": "0.21.4",
    "check-env": "1.3.0",
    "classnames": "2.3.2",
    "cspell": "6.31.1",
    "date-fns": "2.29.3",
    "debounce": "1.2.1",
    "js-cookie": "3.0.1",
    "path-to-regexp": "6.2.1",
    "rc-tooltip": "6.0.1",
    "react": "^18.2.0",
    "react-datepicker": "4.11.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "3.1.4",
    "react-hook-form": "7.44.3",
    "react-number-format": "5.2.2",
    "react-popper-tooltip": "4.4.2",
    "react-router-dom": "6.13.0",
    "react-toastify": "9.1.2",
    "react-tooltip": "4.5.1",
    "react-use": "17.4.0",
    "sass": "1.60.0",
    "serve": "12.0.1",
    "styled-components": "5.3.9",
    "tailwindcss": "3.3.1",
    "utility-types": "3.10.0",
    "zustand": "4.3.6"
  },
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "eject": "craco eject",
    "pretest:e2e": "rm -rf .coverage:e2e/ .nyc_output/",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --maxWorkers=2",
    "test:e2e": "npx nyc --reporter=lcov --reporter=text-summary playwright test",
    "lint": "npm run lint:src && npm run lint:e2e",
    "lint:ci": "npm run lint:src:ci && npm run lint:e2e:ci",
    "test:e2e:update-snapshots": "npx playwright test --update-snapshots",
    "lint:src": "npx eslint src --ext ts,tsx",
    "lint:src:ci": "npx eslint src --ext ts,tsx -f json --output-file ./.src.eslint.sonar.json || true",
    "lint:e2e": "npx eslint e2e --ext ts,tsx",
    "lint:e2e:ci": "npx eslint e2e --ext ts,tsx -f json --output-file ./.e2e.eslint.sonar.json || true",
    "lint:fix": "npm run lint:src -- --fix && npm run lint:e2e -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "check": "npm run prettier && npm run lint && npm run check:spell",
    "check:ci": "npm run prettier && npm run lint:ci && npm run check:spell && npm run check:package",
    "check:env:prod": "check-env API_BASE_URL",
    "check:env:stage": "check-env API_BASE_URL",
    "check:package": "npx npmPkgJsonLint .",
    "check:spell": "cspell \"src/**\"",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{yml,yaml,json,css,scss,html,md}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "prettier --check --write",
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "auto"
  },
  "devDependencies": {
    "@mdx-js/react": "1.6.22",
    "@playwright/test": "1.32.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/js-cookie": "2.2.7",
    "@types/node": "12.20.55",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.2.5",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "babel-plugin-istanbul": "6.1.1",
    "cross-env": "7.0.3",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-check-file": "2.1.0",
    "eslint-plugin-folders": "1.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hook-form": "0.3.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.0",
    "npm-package-json-lint": "6.4.0",
    "nyc": "15.1.0",
    "playwright-test-coverage": "1.2.12",
    "prettier": "2.8.7",
    "react-scripts": "5.0.1",
    "ts-jest": "29.0.5",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "typescript": "5.0.4",
    "typescript-plugin-css-modules": "5.0.0",
    "web-vitals": "1.1.2"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "npm": "8.19.2",
    "node": "18.13.0"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "3.2.4",
    "npm": "8.19.2"
  }
}
